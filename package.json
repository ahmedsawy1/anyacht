    "xcode": "xed -b ios",
    "start-port": "react-native start --port 8080",
    "android-port": "react-native run-android --port 8080 --deviceId 0151531S40118D97",
    "pod": "cd ios && pod install && cd ..",
    "r-run": "adb uninstall 'com.faisal49m.anyacht' && react-native run-android",
    "codepush": "appcenter codepush release-react -a sawy22417-gmail.com/anyacht-android -d Production",
